---
cloud_controller_uri: <%= node.cloudfoundry_cloud_controller.server.api_uri %>
service:
  name: mysql
  version: "5.1"
  description: 'MySQL database service'
  plans: ['free']
  tags: ['mysql', 'mysql-5.1', 'relational']
host: localhost
index: 0
token: <%= node.cloudfoundry_common.service_token %>
mbus: nats://<%= node.nats_server.user %>:<%= node.nats_server.password %>@<%= node.cloudfoundry_common.nats_server.host %>:<%= node.nats_server.port %>/
pid: <%= node.cloudfoundry_mysql_service.gateway.pid_file %>
node_timeout: <%= node.cloudfoundry_mysql_service.gateway.node_timeout %>
logging:
  level: <%= node.cloudfoundry_mysql_service.gateway.log_level %>
  file: <%= node.cloudfoundry_mysql_service.gateway.log_file %>
